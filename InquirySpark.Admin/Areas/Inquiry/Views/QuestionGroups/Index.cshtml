@model IEnumerable<InquirySpark.Repository.Database.QuestionGroup>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.GroupOrder)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.QuestionGroupShortNm)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.QuestionGroupNm)
