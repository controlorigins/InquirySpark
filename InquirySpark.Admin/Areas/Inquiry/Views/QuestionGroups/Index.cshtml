            <td>
                @Html.DisplayFor(modelItem => item.GroupFooter)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModifiedId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModifiedDt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DependentQuestionGroupId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DependentMinScore)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DependentMaxScore)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Survey.CompletionMessage)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.QuestionGroupId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.QuestionGroupId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.QuestionGroupId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
