            <th>
                @Html.DisplayNameFor(model => model.ModifiedDt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DependentQuestionGroupId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DependentMinScore)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DependentMaxScore)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Survey)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.GroupOrder)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.QuestionGroupShortNm)
