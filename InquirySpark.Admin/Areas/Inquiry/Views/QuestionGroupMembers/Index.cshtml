@model IEnumerable<InquirySpark.Repository.Database.QuestionGroupMember>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.QuestionWeight)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DisplayOrder)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ModifiedId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ModifiedDt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Question)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.QuestionGroup)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.QuestionWeight)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DisplayOrder)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModifiedId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModifiedDt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Question.QuestionDs)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.QuestionGroup.QuestionGroupNm)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.QuestionGroupMemberId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.QuestionGroupMemberId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.QuestionGroupMemberId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
