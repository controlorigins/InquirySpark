@model IEnumerable<InquirySpark.Repository.Database.ChartSetting>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SiteUserId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SiteAppId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SettingType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SettingName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SettingValue)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SettingValueEnhanced)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateCreated)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastUpdated)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SiteUserId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SiteAppId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SettingType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SettingName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SettingValue)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SettingValueEnhanced)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateCreated)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastUpdated)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
