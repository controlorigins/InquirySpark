@model IEnumerable<InquirySpark.Repository.Database.Role>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.RoleCd)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RoleNm)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RoleDs)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ReviewLevel)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ReadFl)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UpdateFl)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ModifiedId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ModifiedDt)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RoleCd)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RoleNm)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RoleDs)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ReviewLevel)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ReadFl)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UpdateFl)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModifiedId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModifiedDt)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.RoleId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.RoleId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.RoleId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
